apiVersion: v1
kind: Config
clusters:
- name: ${cluster_name}
  cluster:
    server: ${server}
    certificate-authority-data: ${kube_ca_cert}
users:
- name: kubelet
  user:
    token: ${bootstrap_token_id}.${bootstrap_token_secret}
contexts:
- context:
    cluster: ${cluster_name}
    user: kubelet
